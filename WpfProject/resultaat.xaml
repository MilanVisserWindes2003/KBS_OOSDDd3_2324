      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfProject" xmlns:business="clr-namespace:Business;assembly=Business" d:DataContext="{d:DesignInstance Type=business:Business}"
